# Ignore configuration files that may contain sensitive information.
docroot/sites/*/*settings*.php
docroot/sites/*/local*.yml

# Ignore paths that contain generated content.
docroot/files/
docroot/sites/*/files
docroot/sites/*/private

# Ignore default text files
#robots.txt
/CHANGELOG.txt
/COPYRIGHT.txt
/INSTALL*.txt
/LICENSE.txt
/MAINTAINERS.txt
/UPGRADE.txt
/README.txt
docroot/sites/README.txt
docroot/sites/all/modules/README.txt
docroot/sites/all/themes/README.txt

# Ignore the extra 'vendor' folder.
/docroot/vendor

# Ignore everything but the "sites" folder ( for non core developer )
#.htaccess
#web.config
#authorize.php
#cron.php
#index.php
#install.php
#update.php
#xmlrpc.php
#/includes
#/misc
#/modules
#/profiles
#/scripts
#/themes
